import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner myObj = new Scanner(System.in);
		int n = myObj.nextInt();
		for(int i = 0; i<2*n; i++){
		    if(i<n){
		        for(int j=0;j<i;j++){
		            System.out.print("*");
		        }
		    }
		    else{
		        for(int j=0;j<2*n-i;j++){
		            System.out.print("*");
		        }
		    }
		    System.out.println();
		}
	}
}
